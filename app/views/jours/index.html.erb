<p id="notice"><%= notice %></p>

<h1>Listing Jours</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jours.each do |jour| %>
      <tr>
        <td><%= link_to 'Show', jour %></td>
        <td><%= link_to 'Edit', edit_jour_path(jour) %></td>
        <td><%= link_to 'Destroy', jour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jour', new_jour_path %>
